prototype(Gmedia.CoreSite:Container) < prototype(Neos.Neos:Content) {
	width = ${q(node).property('container')}
	colspacing = ${q(node).property('colspacing')}
	vspacing = ${q(node).property('vspacing')}
    content = Neos.TypoScript:Collection {
		collection = ${q(node)}
		attributes.class = 'row'
		itemRenderer = Neos.Neos:ContentCollection {
			nodePath = '.'
		}
		itemName = 'node'
	}
	bgcolor = ${q(node).property('bgColor')}
	bgimage = ${q(node).property('bgImage')}
	bgposition = ${q(node).property('bgPosition')}
	additionalClasses = ${q(node).property('additionalClasses')}
	
	prototype(Neos.Neos:Content).attributes.class = 'col-12'
}